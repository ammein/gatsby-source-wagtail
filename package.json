{
  "name": "gatsby-source-wagtail",
  "description": "Gatsby plugin which adds Wagtail Grapple support to Gatsby",
  "version": "0.5.6",
  "author": "NathHorrigan <NathHorrigan@gmail.com>",
  "bugs": {
    "url": "https://github.com/NathHorrigan/gatsby-source-wagtail/issues"
  },
  "main": "index.js",
  "files": [
    "*.js",
    "babel-plugin-remove-graphql-queries/*.js",
    "index.d.ts"
  ],
  "dependencies": {
    "@babel/runtime": "^7.25.6",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "babel-plugin-remove-graphql-queries": "^5.13.1",
    "fs-extra": "^11.2.0",
    "gatsby": "^2.13.77",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-source-graphql": "^5.13.1",
    "graphql": "^16.9.0",
    "graphql-2-json-schema": "^0.10.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^9.0.1",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "query-string": "^9.1.0",
    "react": "^18.3.1",
    "subscriptions-transport-ws": "^0.11.0",
    "traverse": "^0.6.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "babel-preset-gatsby-package": "^3.13.1",
    "cross-env": "^7.0.3",
    "prettier": "3.3.3"
  },
  "homepage": "https://github.com/NathHorrigan/gatsby-source-wagtail//#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "graphql",
    "universal"
  ],
  "peerDependencies": {
    "gatsby": "^2.13.77"
  },
  "resolutions": {
    "graphql": "14.5.0"
  },
  "license": "MIT",
  "repository": "https://github.com/NathHorrigan/gatsby-source-wagtail/",
  "scripts": {
    "build": "babel src/ --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write src/**/*.js",
    "clean": "rm -rf ./*.js"
  }
}
